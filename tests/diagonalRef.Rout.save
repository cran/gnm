> library(gnm)
> set.seed(1)
> data(voting)
> 
> count <- with(voting, percentage/100 * total)
> yvar <- cbind(count, voting$total - count)
> 
> classMobility <- gnm(yvar ~ Nonlin(Dref(origin, destination)), 
+                      family = binomial, data = voting)
