> upward <- with(voting, origin != 1 & destination == 1)
> downward <- with(voting, origin == 1 & destination != 1)
> 
> socialMobility <- gnm(yvar ~ Dref(origin, destination,
+                                   delta = ~ 1 + downward + upward),
+                       family = binomial, data = voting)
